{
    "name": "hz-hbo-ict/laravel-core-ui",
    "description": "Very opinionated implementation of CoreUI within Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Loek van der Linde",
            "email": "loek.linde@hz.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "HzHboIct\\LaravelCoreUI\\": "src/"
        }
